@{
    Layout = "~/Views/Layouts/_Overstory.cshtml";
}


<script src="~/Resource/Scripts/JqueryFn.js"></script>
  
    <object id="webofficeword" height="400px" width="100%" style="LEFT: 0px; TOP:0px" classid="clsid:E77E049B-23FC-4DB8-B756-60529A35FAD5" codebase="weboffice_v6.0.5.0.cab#Version=6,0,5,0">
        <param name="_ExtentX" value='6350'>
        <param name='_ExtentY' value='6350'>
    </object>


    <script type="text/javascript">
        var loadDocument = function (url) {//doc xls wps 
            var webObj = document.getElementById("webofficeword");
            var type = "@Request.Params["type"]";
          
            webObj.LoadOriginalFile(url,type);
        };
        $(function () {
            $("#webofficeword").width($(document).width() - 4).height($(document).height() - 40);
            var pid = "@Request.Params["pid"]";
            var typeId = "@Request.Params["typeid"]";
            $.Json("/teacher/showoffice", "GetUrl", {
                pid: pid,
                typeId: typeId
            },function(url) {
                url = "http://"+location.host + url;
                loadDocument(url);
            });

        });
    </script>